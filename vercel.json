{
    "version": 2,
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "builds": [
        {
            "src": "dist/vercel.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/dist/vercel.js"
        }
    ]
}
