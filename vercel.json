{
    "version": 2,
    "builds": [
        {
            "source": "src/app.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**"]
            }
        }
    ],
    "routes": [
        {
            "source": "/(.*)",
            "destination": "dist/app.js"
        }
    ]
}
