{
    "version": 2,
    "builds": [{
        "src": "server",
        "use": "@vercel/node"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/index.js"
        }
    ],
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "buildCommand": "rimraf ./dist",
    "devCommand": "node server/index.js"
}

