{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "outputDirectory": "dist",
    "installCommand": "npm install --legacy-peer-deps",
    "buildCommand": "rimraf ./dist && tsc && tsc-alias && node build.js && cp -r src/public dist/",
    "devCommand": "node dist/index.js"
}
