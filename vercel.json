{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "outputDirectory": "dist",
    "framework": "express",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "dist/app.cjs"
        }
    ],
    "functions": {
        "dist/**/*": {
            "maxDuration": 60
        }
    }
}
