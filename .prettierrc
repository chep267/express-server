{
    "$schema": "https://json.schemastore.org/prettierrc",
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,

    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 126,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "endOfLine": "auto",
    "useTabs": false
}
